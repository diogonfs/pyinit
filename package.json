{
	"name": "pyinit",
	"displayName": "PyInit",
	"description": "Automatic generation of python __init__.py files and import statements within folders",
	"version": "0.0.1",
	"publisher": "Diogo Nolasco",
	"engines": {
		"vscode": "^1.36.0"
	},
	"author": {
		"name": "Diogo Nolasco"
	},
	"categories": [
		"Formatters",
		"Other"
	],
	"keywords": [
		"python",
		"init",
		"generator"
	],
	"icon": "src/images/piinit_icon.png",
	"activationEvents": [
		"onCommand:extension.generateInit"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.generateInit",
				"title": "Generate Init",
				"when": "false"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "extension.generateInit",
					"group": "navigation"
				}
			]
		},
		"keybindings": [
			{
			  "command": "extension.generateInit",
			  "key": "ctrl+i",
			  "mac": "cmd+i",
			  "when": "explorerResourceIsFolder"
			}
		  ]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/diogonfs/pyinit.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.14.12",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0"
	}
}